{
  "name": "dvc-blog",
  "private": true,
  "description": "Blog for dvc.org",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "start": "yarn develop",
    "typecheck": "tsc --noEmit",
    "lint-ts": "tslint --project .",
    "lint-css": "stylelint \"src/**/*.css\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier --write",
      "yarn lint-ts",
      "git add"
    ],
    "*.md": [
      "yarn prettier --write",
      "git add"
    ],
    "*.css": [
      "yarn stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@reach/portal": "^0.4.0",
    "@reach/tooltip": "^0.4.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.17.0",
    "gatsby-image": "^2.2.29",
    "prismjs": "^1.17.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@svgr/webpack": "^4.3.3",
    "@types/classnames": "^2.2.9",
    "@types/react-dom": "^16.9.3",
    "@types/react-helmet": "^5.0.12",
    "autoprefixer": "^9.6.5",
    "eslint-plugin-graphql": "^3.1.0",
    "gatsby-plugin-feed": "^2.3.19",
    "gatsby-plugin-google-analytics": "^2.1.23",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-netlify-cms": "^4.1.25",
    "gatsby-plugin-postcss": "^2.1.13",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-remark-copy-linked-files": "^2.1.28",
    "gatsby-remark-images": "^3.1.28",
    "gatsby-remark-prismjs": "^3.3.20",
    "gatsby-remark-responsive-iframe": "^2.2.25",
    "gatsby-remark-smartypants": "^2.1.14",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-remark": "^2.6.30",
    "gatsby-transformer-sharp": "^2.3.0",
    "graphql": "^14.5.8",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "netlify-cms-app": "^2.9.7",
    "postcss-custom-media": "^7.0.8",
    "postcss-mixins": "^6.2.2",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.18.2",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.4"
  }
}
